                "settings": {
                    "foreground": "#808000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "variable.language",
                "settings": {
                    "foreground": "#ff0000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "variable.name",
                "settings": {
                    "foreground": "#ffffff",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "variable.other",
                "settings": {
                    "foreground": "#808000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "variable.other.readwrite",
                "settings": {
                    "foreground": "#808000",
                    "fontStyle": "normal"

                    "foreground": "#804000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "support",
                "settings": {
                    "foreground": "#808000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "support.class",
                "settings": {
                    "foreground": "#808000",
                    "fontStyle": "normal"
                }

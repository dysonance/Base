{
    // EDITOR CONFIGURATIONS -------------------------------------------------------------------------------------------
    "files.associations": {
        "*.lzz": "cpp"
    },
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Tinacious Contrast",
    "editor.rulers": [
        120
    ],
    "editor.fontFamily": "'PT Mono', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 11,
    "editor.showFoldingControls": "always",

                }
            },
            {
                "scope": "keyword.operator.arithmetic",
                "settings": {
                    "foreground": "#ffff00",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "keyword.operator.logical",
                "settings": {
                    "foreground": "#ffff00",
                    "fontStyle": "normal"

        "badge.background": "#404040",
        "button.background": "#404040",
        "diffEditor.insertedTextBackground": "#004000",
        "diffEditor.insertedTextBorder": "#002000",
        "diffEditor.removedTextBackground": "#400000",
        "diffEditor.removedTextBorder": "#200000",
        "dropdown.background": "#404040",
        "editor.background": "#000040",
        "editor.findMatchBackground": "#004000",
        "editor.findMatchHighlightBackground": "#004000",
        "editor.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#000000",
        "editor.selectionBackground": "#404040",
        "editor.selectionHighlightBackground": "#004000",
        "editorBracketMatch.background": "#0000ff",
        "editorBracketMatch.border": "#00ffff",
        "editorCursor.background": "#ff8000",
        "editorCursor.foreground": "#ff800080",
        "editorError.foreground": "#ff0000",
        "editorGroup.background": "#000000",
        "editorGroup.border": "#ffffff",
        "editorGutter.addedBackground": "#008000",
        "editorGutter.background": "#000030",
        "editorGutter.deletedBackground": "#800000",
        "editorGutter.modifiedBackground": "#808000",
        "editorMarkerNavigationError.background": "#ff0000",
        "editorMarkerNavigationWarning.background": "#ff8000",
        "editorRuler.foreground": "#000080",
        "editorWarning.foreground": "#ff8000",
        "foreground": "#ffffff",
        "input.background": "#404040",
        "list.focusBackground": "#0000ff",
        "panel.background": "#202020",
        "sideBar.background": "#000000",
        "statusBar.background": "#0000ff",
        "tab.activeBackground": "#0000ff",
        "terminal.background": "#000000",
        "tab.inactiveBackground": "#000040",
        "tab.border": "#ffffff",
        "statusBar.border": "#00ffff",
        "sideBar.border": "#ffffff",
        "panel.border": "#ffffff",
        "focusBorder": "#ffffff",
        "input.border": "#808080",
        "titleBar.activeBackground": "#000000"
    },
    // syntax highlighting customizations
    "editor.tokenColorCustomizations": {

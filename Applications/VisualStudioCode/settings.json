                "settings": {
                    "foreground": "#ff0000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "constant.numeric",
                "settings": {
                    "foreground": "#ff0000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "constant.numeric.float",
                "settings": {
                    "foreground": "#ff0000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "constant.numeric.hex",
                "settings": {
                    "foreground": "#ff0000",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "constant.numeric.integer",
                "settings": {
                    "foreground": "#ff0000",
                    "fontStyle": "normal"
                }
            },
            {

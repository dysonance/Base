                "settings": {
                    "foreground": "#ffff00",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "punctuation.separator.continuation",
                "settings": {
                    "foreground": "#ffff00",
                    "fontStyle": "normal"
                }
            },
            {
                "scope": "punctuation.definition.string.begin",
                "settings": {
                    "foreground": "#00ff00",
                    "fontStyle": "normal"
                }
            },
            {
